<div class="container">
	<div class="row">
		<div class="w12">
			<a href="/teams">Back to all teams</a>
		</div>
	</div>
	<div class="row">
		<div class="w12 wysiwyg">
			<h1>Editing <%= team.teamName %></h1>
			<form action="/teams/update/<%= team.id %>" method="POST">
				<textarea name="teamInfo" id="teamInfo" cols="30" rows="10"></textarea>
				<input type="submit" class="cta-submit block" value="Save changes">
			</form>
		</div>
	</div>
</div>