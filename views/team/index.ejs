<div class="container">
	<div class="row">
		<div class="w12 wysiwyg">
			<h1>Teams</h1>
			<div class="row">
				<% _.each(teams, function(team, i, teams) { %>
				<div class="w4 team-box<% if ((i+1) % 3 == 0) { %> last<% } %>">
					<h2><a href="/teams/<%= team.id %>"><%= team.teamName %></a></h2>
				</div>
				<% }) %>	
			</div>
		</div>	
	</div>
</div>